# Configuración de la Aplicación
ENTORNO=desarrollo
PUERTO_APLICACION=8000

# Base de Datos PostgreSQL
POSTGRES_USER=usuario_db
POSTGRES_PASSWORD=clave_segura
POSTGRES_DB=mi_base_datos

# URLs de Conexión
URL_BASE_DATOS=postgresql://usuario_db:clave_segura@base_datos:5432/mi_base_datos
URL_REDIS=redis://cache_redis:6379

# Redis (opcional)
# REDIS_PASSWORD=clave_redis_segura

# Nginx
NGINX_HTTP_PORT=8080
NGINX_HTTPS_PORT=8443

# Configuración de Producción (cambiar en producción)
# POSTGRES_PASSWORD=contraseña_super_segura_produccion
# REDIS_PASSWORD=contraseña_redis_produccion
